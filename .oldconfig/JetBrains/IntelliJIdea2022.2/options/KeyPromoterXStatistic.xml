<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'Ctrl+C' or 'Ctrl+Insert'</ShortCut>
          <Description>Copy</Description>
          <Count>6</Count>
          <Hit>3</Hit>
        </Item>
      </Statistic>
      <Statistic Action="Cut">
        <Item>
          <IdeaActionID>$Cut</IdeaActionID>
          <ShortCut>'Ctrl+X' or 'Shift+Delete'</ShortCut>
          <Description>Cut</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Desktop Directory">
        <Item>
          <IdeaActionID>FileChooser.GotoDesktop</IdeaActionID>
          <ShortCut>'Ctrl+D'</ShortCut>
          <Description>Desktop Directory</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Disable Completions">
        <Item>
          <IdeaActionID>copilot.disableCopilot</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+O'</ShortCut>
          <Description>Disable Completions</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Enable Completions">
        <Item>
          <IdeaActionID>copilot.enableCopilot</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+O'</ShortCut>
          <Description>Enable Completions</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste">
        <Item>
          <IdeaActionID>$Paste</IdeaActionID>
          <ShortCut>'Ctrl+V' or 'Shift+Insert'</ShortCut>
          <Description>Paste</Description>
          <Count>11</Count>
          <Hit>2</Hit>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'Alt+1'</ShortCut>
          <Description>Project</Description>
          <Count>4</Count>
          <Hit>11</Hit>
        </Item>
      </Statistic>
      <Statistic Action="Scratch File">
        <Item>
          <IdeaActionID>NewScratchFile</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+Insert'</ShortCut>
          <Description>Scratch File</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>6</Count>
          <Hit>3</Hit>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'game'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'game'</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>